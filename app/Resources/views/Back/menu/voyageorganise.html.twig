<li  {% if app.request.getPathInfo|split('/admin/voyage_organise')|length > 1 %} class="active open"{% endif %}>
    <a href="javascript:void(0)"><i class="clip-map"></i>
	<span class="title">Voyage Organisé</span><i class="icon-arrow"></i>
	<span class="selected"></span>
    </a>
    <ul class="sub-menu">
	<li {% if app.request.get('_route') in ['back_voyages_organises_reservation_validation','back_voyages_organises_reservation_ajouter','back_voyages_organises_reservation','back_voyages_organises_reservation_consulter'] %} class="active open"{% endif %} >
	    <a href="javascript:void(0)"><i class="clip-earth-2"></i>
		<span class="title">Réservations</span><i class="icon-arrow"></i>
	    </a>
	    <ul class="sub-menu">
		<li {% if app.request.get('_route')=='back_voyages_organises_reservation' %} class="open"{% endif %}>
		    <a href="{{ path('back_voyages_organises_reservation') }}">
			<span class="title">Liste</span>
		    </a>
		</li>
	    </ul>
	    <ul class="sub-menu">
		<li {% if app.request.get('_route')=='back_voyages_organises_reservation_ajouter' %} class="open"{% endif %}>
		    <a href="{{ path('back_voyages_organises_reservation_ajouter') }}">
			<span class="title">Nouvelle</span>
		    </a>
		</li>
	    </ul>
	</li>
	<li {% if app.request.get('_route') in ['back_voyages_organises_destination'] %} class="active open"{% endif %} >
	    <a href="javascript:void(0)"><i class="clip-list"></i>
		<span class="title">Réferentiel</span><i class="icon-arrow"></i>
	    </a>
	    <ul class="sub-menu">
		<li {% if app.request.get('_route')=='back_voyages_organises_destination' %} class="open"{% endif %}>
		    <a href="{{ path('back_voyages_organises_destination') }}">
			<span class="title">Destination</span>
		    </a>
		</li>
	    </ul>
	</li>
	<li {% if app.request.get('_route') in ['back_vo_liste','back_vo_ajouter','back_vo_photos','back_vo_descriptions'] %} class="active open"{% endif %} >
	    <a href="javascript:void(0)"><i class="clip-world"></i>
		<span class="title">Voyages Organisés</span><i class="icon-arrow"></i>
	    </a>
	    <ul class="sub-menu">
		<li {% if app.request.get('_route')=='back_vo_liste' %} class="open"{% endif %}>
		    <a href="{{ path('back_vo_liste') }}">
			<span class="title">Liste</span>
		    </a>
		</li>
	    </ul>
	</li>
    </ul>
</li>