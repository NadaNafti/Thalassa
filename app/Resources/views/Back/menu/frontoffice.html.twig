<li  {% if app.request.getPathInfo|split('/admin/FrontOffice')|length > 1 %} class="active open"{% endif %}>
    <a href="javascript:void(0)"><i class="clip-screen"></i>
	<span class="title">FrontOffice</span><i class="icon-arrow"></i>
	<span class="selected"></span>
    </a>
    <ul class="sub-menu">
	<li {% if app.request.getPathInfo|split('/admin/FrontOffice/VoyageOrganise')|length > 1 %} class="active open"{% endif %} >
	    <a href="javascript:void(0)">
		<span class="title">Voyage Organisé</span><i class="icon-arrow"></i>
	    </a>
	    <ul class="sub-menu">
		<li {% if app.request.get('_route')in ['front_config_voyageorganise_topdestination'] %} class="open"{% endif %}>
		    <a href="{{ path('front_config_voyageorganise_topdestination') }}">
			<span class="title">Top Destination</span>
		    </a>
		</li>
	    </ul>
	    <ul class="sub-menu">
		<li {% if app.request.get('_route')in ['front_config_voyageorganise_slider'] %} class="open"{% endif %}>
		    <a href="{{ path('front_config_voyageorganise_slider') }}">
			<span class="title">Slider</span>
		    </a>
		</li>
	    </ul>
	    <ul class="sub-menu">
		<li {% if app.request.get('_route')in ['front_config_voyageorganise_blockmetro'] %} class="open"{% endif %}>
		    <a href="{{ path('front_config_voyageorganise_blockmetro') }}">
			<span class="title">Idées Voyages</span>
		    </a>
		</li>
	    </ul>
	</li>
    </ul>
</li>