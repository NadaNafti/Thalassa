{% import ":Back:macros.html.twig" as macros %}
{% import ":Back:form.html.twig" as formulaire %}
<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta charset="utf-8" />
        <title>{% block title %}Tunisia Thalassa Travel{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('fav.ico') }}" />
        {% block meta %}
            <meta name="keywords" content=""/>
            <meta name="description" content="">
            <meta name="author" content="">
            <meta name="viewport" content="width=device-width, initial-scale=1"/>
        {% endblock %}
        {% block stylesheets %}
            <!-- GOOGLE FONTS -->
            <link href='http://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700' rel='stylesheet' type='text/css'>
            <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,300,600' rel='stylesheet' type='text/css'>
            <!-- /GOOGLE FONTS -->
            <link rel="stylesheet" type="text/css" href="{{ asset('front-assets/css/bootstrap.css') }}" media="screen">
            <link rel="stylesheet" type="text/css" href="{{ asset('front-assets/css/font-awesome.css') }}" media="screen" />
            <link rel="stylesheet" type="text/css" href="{{ asset('front-assets/css/icomoon.css') }}" media="screen" />
            <link rel="stylesheet" type="text/css" href="{{ asset('front-assets/css/styles.css') }}" media="screen" />
            <link rel="stylesheet" type="text/css" href="{{ asset('front-assets/css/mystyles.css') }}" media="screen" />
            <link rel="stylesheet" type="text/css" href="{{ asset('front-assets/css/schemes/mandy.css') }}" media="screen" />
            <script src="{{ asset('front-assets/js/modernizr.js') }}"></script>
        {% endblock %}
        <script src="http://maps.google.com/maps/api/js?sensor=false"></script>
    </head>

    <body class="boxed">
        <!-- FACEBOOK WIDGET -->
        <div id="fb-root"></div>
        <script>
            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id))
                    return;
                js = d.createElement(s);
                js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.0";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>
        <!-- /FACEBOOK WIDGET -->
        <div class="global-wrap">
            <header id="main-header">
                {% include ':Front:topnav.html.twig' %}
                {% include ':Front:menu.html.twig' %}
            </header>
        {% block body %}{% endblock %}
        {% include ':Front:footer.html.twig' %}
        {% block javascripts %}
            <script src="{{ asset('front-assets/js/jquery.js')}}"></script>
            <script src="{{ asset('front-assets/js/bootstrap.js')}}"></script>
            <script src="{{ asset('front-assets/js/slimmenu.js')}}"></script>
            <script src="{{ asset('front-assets/js/bootstrap-datepicker.js')}}"></script>
            <script src="{{ asset('front-assets/js/bootstrap-timepicker.js')}}"></script>
            <script src="{{ asset('front-assets/js/nicescroll.js')}}"></script>
            <script src="{{ asset('front-assets/js/dropit.js')}}"></script>
            <script src="{{ asset('front-assets/js/ionrangeslider.js')}}"></script>
            <script src="{{ asset('front-assets/js/icheck.js')}}"></script>
            <script src="{{ asset('front-assets/js/fotorama.js')}}"></script>
            <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
            <script src="{{ asset('front-assets/js/typeahead.js')}}"></script>
            <script src="{{ asset('front-assets/js/card-payment.js')}}"></script>
            <script src="{{ asset('front-assets/js/magnific.js')}}"></script>
            <script src="{{ asset('front-assets/js/owl-carousel.js')}}"></script>
            <script src="{{ asset('front-assets/js/fitvids.js')}}"></script>
            <script src="{{ asset('front-assets/js/tweet.js')}}"></script>
            <script src="{{ asset('front-assets/js/countdown.js')}}"></script>
            <script src="{{ asset('front-assets/js/gridrotator.js')}}"></script>
            <script src="{{ asset('front-assets/js/custom.js')}}"></script>
        {% endblock %}
            {% block jquery %}{% endblock %}
    </div>
</body>
</html>
